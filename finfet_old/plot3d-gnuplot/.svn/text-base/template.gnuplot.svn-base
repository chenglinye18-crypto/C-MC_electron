set terminal postscript eps enhanced color
set format z

set size 0.5,0.5

set style line 1 lt 1 lc rgb '#000000' lw 0.01 pt -1 ps 0.1
set style line 2 lt 1 lc rgb 'black' lw 1 pt -1 ps 1 

set view 51, 40, 0.95, 1.2
#set label "Vg=Vd=1V," at -28,0,1.7
#set label "noqc" at -23,0,1.55
#set label "Potential (V)" at -45,-0.5,0.65 rotate left
#set label "z (nm)" at -38, 3, 0.25
#set label "y (nm)" at -5, 12, 0.3
set xtics in offset 0.8,-0.3,0
set ytics in offset 0,-0.45,0
set ztics in offset 0.7,0,0
unset key
#set pm3d at s explicit
#set pm3d hidden3d 1
#set palette defined(0 "black", 1 "black")
set palette defined(0 0.1 0.1 0.1, 1 0.1 0.1 0.1)
#set palette defined(0 "blue", 1 "blue")
#set palette defined(0 "red", 1 "red")
#set palette defined(0 0.1 0.1 0.1, 1 0.3 0.3 0.3)
#set palette defined(0 "white", 1 "white")
#set palette defined(0 0.6 0.6 0.6, 1 0.95 0.95 0.95)
#set palette gamma 0.6
set samples 30; set isosamples 30
set grid layerdefault
set ticslevel 0
unset colorbox

